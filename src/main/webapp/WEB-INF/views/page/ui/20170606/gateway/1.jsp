<script>

    //模拟select下拉框
    $('.nice-select').click(function(e){
        $(this).find('ul').toggle();
        e.stopPropagation();
    });
    $(document).click(function(){
        $('.nice-select ul').hide();
    });

    //新闻动态tab切换
    $(".js-news-nav").on('click',function () {
        $(".js-news-nav").removeClass("active");
        $(this).addClass("active");
        var navSwiper = new Swiper('.swiper-info', {
            //只能使用扩展API函数改变slides滑动
            onlyExternal: true,
            //滑动方向，可设置水平(horizontal)或垂直(vertical)。
            mode: 'horizontal'
        });
        navSwiper.swipeTo($(this).data('val'), 300, false);
    });


    //图片新闻-图片轮播切换
    var mySwiper = new Swiper('.swiper-news',{
        onlyExternal : true,
        autoplay: 3000,
        loop: true,
        speed: 500
    });
    $('.swipe-prev').click(function(){
        mySwiper.swipePrev();
    });
    $('.swipe-next').click(function(){
        mySwiper.swipeNext();
    });

    //ie低版本兼容placeholder
    $('.s-txt').placeholder();

</script>
